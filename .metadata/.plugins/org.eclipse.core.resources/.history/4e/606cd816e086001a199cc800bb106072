package numberRanger;

import java.util.Collection;

/*Assumptions: Empty Collections should result is regarded as Invalid input.
 *            Input of n,n+1 should result in a range of "n-n+1"
 *            Input contains no repeats. 
 *            Any other characters(Can not be parsed to Ints) will be regarded as Invalid input
 * */
public class Main {

	public static void main(String[] args) {

		NumbersConveter obj = new NumbersConveter();
		NumbersConveter obj2 = new NumbersConveter();

		Collection<Integer> coll = obj2.collect("3");
		System.out.println(obj2.summarizeCollection(coll));
		// System.out.println((obj.collect("")));
		// System.out.println(obj2.summarizeCollection(obj2.collect("1,3,4,5,6")));

	}
}
